{
  "name": "@sylphx/zencss",
  "version": "0.1.0",
  "description": "Type-safe CSS-in-TypeScript without codegen. 38-2100% smaller bundles than Tailwind and Panda CSS.",
  "keywords": [
    "css",
    "css-in-js",
    "typescript",
    "atomic-css",
    "zero-runtime",
    "type-safe",
    "tailwind",
    "panda-css",
    "critical-css",
    "performance"
  ],
  "author": "SylphX Ltd",
  "license": "MIT",
  "homepage": "https://sylphx.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/sylphxltd/zencss.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/sylphxltd/zencss/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vitest": "^1.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
